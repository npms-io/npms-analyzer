{
  "name": "npms-analyzer",
  "version": "0.1.0",
  "description": "The analyzer behind npms.io",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '{*.js,lib/**/*.js,test/**/*.js}' --ignore-pattern=test/coverage",
    "test": "test/download && VCR_MODE=cache mocha",
    "test-cov": "test/download && VCR_MODE=cache istanbul cover --dir test/coverage _mocha && echo Coverage lies in test/coverage/lcov-report/index.html",
    "test-travis": "test/download && VCR_MODE=playback istanbul cover _mocha --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "bugs": "https://github.com/npms-io/npms-analyzer/issues/",
  "repository": {
    "type": "git",
    "url": "git://github.com/npms-io/npms-analyzer.git"
  },
  "keywords": [
    "npm",
    "npms",
    "npmsearch",
    "npmsearch",
    "search",
    "node",
    "module",
    "package"
  ],
  "author": "Andr√© Cruz <amdfcruz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.5.0",
    "bignumber.js": "^3.0.0",
    "bluebird": "^3.1.1",
